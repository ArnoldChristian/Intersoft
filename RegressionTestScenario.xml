<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="RMG Returns">
  <listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
  </listeners>
  <test name="Regression">
    <classes>
      <class name="com.ReturnsPortal.OrderFoundWithItemData.OrderFoundWithItemData"/>
      <class name="com.ReturnsPortal.OrderFoundWithItemData.OrderFoundWithMultipleItemData"/>
      <class name="com.ReturnsPortal.OrderFoundWithoutItemData.OrderFoundWithOutItemData"/>
      <class name="com.ReturnsPortal.OrderNotFound.OrderNotFound"/>
    </classes>
  </test> <!-- Regression -->
</suite> <!-- RMGReturns -->
